{
  "expo": {
    "name": "HyperFit",
    "slug": "hyperfit",
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f172a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hyperfit.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSHealthShareUsageDescription": "HyperFit needs access to your health data to track your fitness progress.",
        "NSHealthUpdateUsageDescription": "HyperFit needs to update your health data to sync your workouts."
      },
      "scheme": "hyperfit"
    },
    "android": {
      "package": "com.hyperfit.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0f172a"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACTIVITY_RECOGNITION",
        "ACCESS_FINE_LOCATION"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "hyperfit"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          },
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 23
          }
        }
      ],
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "637e7157-3eb7-4677-baa1-d32c3f60f509"
      }
    }
  }
}
